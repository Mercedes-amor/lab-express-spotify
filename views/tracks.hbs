<div class="principalDiv">
    <h1>Tracks</h1>

    {{#each tracksCollection}}
        <div class="trackCard">
            <h3>{{this.name}}</h3>
            <audio controls>
            <source src="{{this.preview_url}}" type="audio/mp3">       
            </audio> 
        </div>
    {{/each}}

</div>